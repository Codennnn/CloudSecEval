{
  "name": "nestjs-docs-cn-monorepo",
  "private": true,
  "packageManager": "pnpm@9.15.3",
  "engines": {
    "node": ">=22"
  },
  "workspaces": [
    "apps/*"
  ],
  "scripts": {
    "dev": "pnpm --filter nest-js-docs-cn dev",
    "build": "pnpm --filter nest-js-docs-cn build",
    "start": "pnpm --filter nest-js-docs-cn start",
    "lint": "pnpm --filter nest-js-docs-cn lint"
  }
}
