{
  "name": "@mono/utils",
  "version": "0.1.0",
  "private": true,
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rimraf dist && tsup",
    "lint": "pnpm lint:es && pnpm lint:ts",
    "lint:es": "eslint \"**/*.{ts,tsx,js,mjs,cjs}\"",
    "lint:ts": "tsc --noEmit --skipLibCheck",
    "format": "eslint \"**/*.{ts,tsx,js,mjs,cjs}\" --fix"
  },
  "devDependencies": {
    "tsup": "^8.3.0"
  }
}
